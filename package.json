{
  "name": "scribnet",
  "version": "1.0.0",
  "description": "Yet Another Notes App",
  "main": "./app/main.mjs",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "build-static": "DEV_STATIC_ONLY=true npx @11ty/eleventy",
    "clean": "rm -r ./site/* && touch views/index.njk",
    "scrivener": "DEVELOPMENT=true node scrivener/main.mjs",
    "server": "node scrivener/main.mjs",
    "test": "mocha test/**/*.mjs",
    "build-serve": "npx @11ty/eleventy --serve",
    "build-watch": "npx @11ty/eleventy --watch",
    "build-watch-flexible": "DEV_FETCH_FAILS_BUILD=false npx @11ty/eleventy --watch",
    "static": "DEV_FETCH_FAILS_BUILD=false npx @11ty/eleventy --watch",
    "reload-db": "cp data-folder/dbfile.bkp data-folder/dbfile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hfarr/scribnet.git"
  },
  "keywords": [],
  "author": "Ohmsec",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hfarr/scribnet/issues"
  },
  "homepage": "https://github.com/hfarr/scribnet#readme",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "axios": "^0.24.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.2",
    "express-graphql": "^0.12.0",
    "graphql": "^15.3.0"
  },
  "devDependencies": {
    "mocha": "^8.1.3"
  }
}
